<h1>Listing hr_config_religions</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Title</th>
      <th>Khtitle</th>
      <th>Description</th>
      <th>Khdescription</th>
      <th>Usercreate</th>
      <th>Userupdate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hr_config_religions.each do |hr_config_religion| %>
      <tr>
        <td><%= hr_config_religion.code %></td>
        <td><%= hr_config_religion.title %></td>
        <td><%= hr_config_religion.khtitle %></td>
        <td><%= hr_config_religion.description %></td>
        <td><%= hr_config_religion.khdescription %></td>
        <td><%= hr_config_religion.usercreate %></td>
        <td><%= hr_config_religion.userupdate %></td>
        <td><%= link_to 'Show', hr_config_religion %></td>
        <td><%= link_to 'Edit', edit_hr_config_religion_path(hr_config_religion) %></td>
        <td><%= link_to 'Destroy', hr_config_religion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Religion', new_hr_config_religion_path %>
