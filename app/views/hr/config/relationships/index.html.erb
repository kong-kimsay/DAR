<h1>Listing hr_config_relationships</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Title</th>
      <th>Khtitle</th>
      <th>Description</th>
      <th>Khdescription</th>
      <th>Usercreate</th>
      <th>Userupdate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hr_config_relationships.each do |hr_config_relationship| %>
      <tr>
        <td><%= hr_config_relationship.code %></td>
        <td><%= hr_config_relationship.title %></td>
        <td><%= hr_config_relationship.khtitle %></td>
        <td><%= hr_config_relationship.description %></td>
        <td><%= hr_config_relationship.khdescription %></td>
        <td><%= hr_config_relationship.usercreate %></td>
        <td><%= hr_config_relationship.userupdate %></td>
        <td><%= link_to 'Show', hr_config_relationship %></td>
        <td><%= link_to 'Edit', edit_hr_config_relationship_path(hr_config_relationship) %></td>
        <td><%= link_to 'Destroy', hr_config_relationship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Relationship', new_hr_config_relationship_path %>
