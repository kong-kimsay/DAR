<h1>Listing hr_config_educations</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Title</th>
      <th>Khtitle</th>
      <th>Description</th>
      <th>Khdescription</th>
      <th>Usercreate</th>
      <th>Userupdate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hr_config_educations.each do |hr_config_education| %>
      <tr>
        <td><%= hr_config_education.code %></td>
        <td><%= hr_config_education.title %></td>
        <td><%= hr_config_education.khtitle %></td>
        <td><%= hr_config_education.description %></td>
        <td><%= hr_config_education.khdescription %></td>
        <td><%= hr_config_education.usercreate %></td>
        <td><%= hr_config_education.userupdate %></td>
        <td><%= link_to 'Show', hr_config_education %></td>
        <td><%= link_to 'Edit', edit_hr_config_education_path(hr_config_education) %></td>
        <td><%= link_to 'Destroy', hr_config_education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

