<h1>Listing hr_config_maritals</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Title</th>
      <th>Khtitle</th>
      <th>Description</th>
      <th>Khdescription</th>
      <th>Usercreate</th>
      <th>Userupdate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hr_config_maritals.each do |hr_config_marital| %>
      <tr>
        <td><%= hr_config_marital.code %></td>
        <td><%= hr_config_marital.title %></td>
        <td><%= hr_config_marital.khtitle %></td>
        <td><%= hr_config_marital.description %></td>
        <td><%= hr_config_marital.khdescription %></td>
        <td><%= hr_config_marital.usercreate %></td>
        <td><%= hr_config_marital.userupdate %></td>
        <td><%= link_to 'Show', hr_config_marital %></td>
        <td><%= link_to 'Edit', edit_hr_config_marital_path(hr_config_marital) %></td>
        <td><%= link_to 'Destroy', hr_config_marital, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

